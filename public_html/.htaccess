#GZIP COMPRESSION

<IfModule mod_deflate.c>

    <FilesMatch "\.(js|css)$">

        SetOutputFilter DEFLATE

    </FilesMatch>

</IfModule>

#END GZIP COMPRESSION

#############################################################

<ifmodule mod_expires.c>

    ExpiresActive On

    ExpiresDefault A3600

    <filesmatch ".(jpg|JPG|gif|GIF|png|css|ico|js)$">

        ExpiresDefault "access plus 7 day"

    </filesmatch>

</ifmodule>

