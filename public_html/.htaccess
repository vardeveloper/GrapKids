<IfModule mod_rewrite.c>

RewriteEngine On



#RewriteCond %{HTTP_HOST} ^grapkids.com

#RewriteRule ^ http://www.grapkids.com%{REQUEST_URI} [L,R=301]



#RewriteCond %{HTTP_HOST} ^grapkids.com.pe

